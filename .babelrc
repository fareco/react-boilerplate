{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 1 versions"]
      },
      "useBuiltIns": "usage"
    }],
    "stage-0",
    "react"

  ],
  "plugins": [
    "react-hot-loader/babel",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}